@model IEnumerable<PTEKNIK.Models.Documents>

@{
    ViewBag.Title = "Dokumantasyon";
    Layout = "~/Views/Shared/_MasterLayout.cshtml";
}

<h2>Dökümanlar</h2>


<div class="col-lg-12">
    @foreach (var item in Model)
    {
        <div class="col-lg-2">
            <label>@item.ACIKLAMA</label><br />
            <a href="../../Dokumantasyonlar/@item.PATH" target="_blank">
                <img src="~/Image/doc.png"  width="50" height="50"/>
            </a>
        </div>


    }
</div>

